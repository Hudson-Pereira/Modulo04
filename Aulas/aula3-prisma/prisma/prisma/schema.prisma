generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Filme {
//nome          tipo         definicioes
  id            Int       @default(autoincrement()) @id //padrao definido automatico
  name          String    @unique //garantir que so vai ter um dado com essas informacoes.
  ano           Int?    
  capa          String 
}